{
    "expo": {
        "name": "EsoAgent",
        "slug": "esoagent-client",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "dark",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#0a0a0a"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.esoagent.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#0a0a0a"
            },
            "package": "com.esoagent.app"
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        }
    }
}